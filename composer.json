{
    "name": "decodelabs/deliverance",
    "description": "Shared data transfer interfaces",
    "type": "library",
    "keywords": [ "io", "transfer" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/exceptional": "^0.5.2",
        "decodelabs/tightrope": "^0.2"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Deliverance\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
